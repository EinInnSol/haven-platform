{
  "name": "haven-platform",
  "version": "1.0.0",
  "description": "AI-powered homeless services platform",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create haven_db",
    "db:init": "wrangler d1 execute haven_db --remote --file=./src/database/schema.sql",
    "db:seed": "wrangler d1 execute haven_db --remote --file=./src/database/seed.sql",
    "db:local:init": "wrangler d1 execute haven_db --local --file=./src/database/schema.sql",
    "db:local:seed": "wrangler d1 execute haven_db --local --file=./src/database/seed.sql"
  },
  "dependencies": {
    "itty-router": "^5.0.0",
    "itty-cors": "^1.0.8"
  },
  "devDependencies": {
    "wrangler": "^3.78.0"
  }
}